{
  "name": "vue-pptx",
  "version": "0.0.2",
  "type": "module",
  "description": "vue项目ppt库",
  "main": "lib/index.umd.js",
  "module": "lib/index.es.js",
  "style": "lib/style.css",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && run-s types build-only",
    "preview": "vite preview",
    "build-only": "vite build --mode production",
    "type-check": "vue-tsc --build --force",
    "types": "vue-tsc -p tsconfig.types.json --declaration --emitDeclarationOnly",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write package/",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.2",
    "path": "^0.12.7",
    "prettier": "^3.0.3",
    "sass": "^1.72.0",
    "typescript": "~5.4.0",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.0",
    "vite-plugin-vue-devtools": "^7.0.18",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  },
  "peerDependencies": {
    "vue": "^3.4.21"
  },
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "vue",
    "ppt",
    "pdf-preview"
  ],
  "author": "wangxuan",
  "license": "MIT",
  "homepage": "https://github.com/wxuanwx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wxuanwx"
  },
  "dependencies": {
    "pptx-preview": "^1.0.7"
  }
}
